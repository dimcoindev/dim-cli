{
  "name": "dim-cli",
  "version": "0.6.19",
  "author": "DIMCoin Developers",
  "description": "DIM-cli is a collection of command line tools for the DIM Ecosystem using the Terminal",
  "keywords": [
    "dimcoin",
    "dimtoken",
    "dim",
    "blockchain",
    "nis",
    "api",
    "accounts",
    "wallets",
    "transactions"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/dimcoin/dim-cli.git"
  },
  "license": "MIT",
  "dependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-plugin-transform-inline-environment-variables": "^6.8.0",
    "babel-preset-es2015": "^6.24.1",
    "chalk": "^1.1.3",
    "commander": "2.11.0",
    "crypto-js": "3.1.9-1",
    "easy-table": "1.1.0",
    "mongoose": "^5.0.4",
    "mongoose-increment": "~0.8.0",
    "nem-sdk": "git://github.com/QuantumMechanics/NEM-sdk.git",
    "prettyjson": "1.2.1",
    "request": "^2.87.0",
    "simple-terminal-menu": "1.1.3",
    "xlsx": "0.10.x"
  },
  "devDependencies": {
    "babel-register": "^6.26.0",
    "browserify": "14.1.0",
    "chai": "^4.1.2",
    "duplexer": "0.1.1",
    "mocha": "^2.4.5",
    "through": "2.3.8",
    "uglify-js": "~2.6.0"
  },
  "scripts": {
    "test": "mocha --compilers js:babel-core/register --recursive tests/",
    "build": "babel core/ sdk/ scripts/ --presets babel-preset-es2015 --out-dir build",
    "postinstall": "npm run build",
    "postversion": "git push origin master && git push --tags",
    "start": "./babel-node dim-cli",
    "clean": "rm -f build/*.js"
  },
  "bin": {
    "dim-cli": "./babel-node dim-cli"
  }
}
